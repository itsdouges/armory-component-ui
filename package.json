    "jsdom": "^11.2.0",
    "jsdom-global": "^3.0.2",
    "less": "^2.7.1",
    "less-loader": "^4.0.5",
    "less-vars-loader": "^1.1.0",
    "lodash": "^4.14.1",
    "lz-string": "^1.4.4",
    "mocha": "^3.5.0",
    "nyc": "^11.1.0",
    "postcss-loader": "^2.0.6",
    "pre-commit": "^1.2.2",
    "proxyquire": "^1.8.0",
